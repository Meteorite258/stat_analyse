default: stat_xilinx clean

############################################################################
# begin: cmd_machine in ./cmd/
##@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
cmd_machine: ./cmd/cmd_machine.c
	gcc -c -g -Wall ./cmd/cmd_machine.c
##@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
# end: cmd_machine in ./cmd/
############################################################################


############################################################################
# begin: hierarchy in ./hierarchy/
##@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
sub_modules: ./hierarchy/sub_modules.c
	gcc -c -g -Wall ./hierarchy/sub_modules.c
##@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
# end: hierarchy in ./hierarchy/
############################################################################


############################################################################
# begin: prime in ./prime/
##@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
stat_lut_xilinx: ./prime/stat_lut_xilinx.c
	gcc -c -g -Wall ./prime/stat_lut_xilinx.c
##@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
# end: prime in ./prime/
############################################################################


############################################################################
# begin: common in ./common/
##@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
comm: ./common/common.c
	gcc -c -g -Wall ./common/common.c

help: ./common/help.c
	gcc -c -g -Wall ./common/help.c
##@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
# end: common in ./common/
############################################################################


############################################################################
# begin: main in ./
##@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
main: ./main.c
	gcc -c -g -Wall ./main.c
##@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
# end: main in ./
############################################################################



stat_xilinx: main cmd_machine sub_modules stat_lut_xilinx comm help
	gcc -o stat_xilinx cmd_machine.o sub_modules.o stat_lut_xilinx.o common.o help.o main.o -lm


clean:
	rm *.o


cleanall: clean
	rm stat_xilinx.exe
